<mapping name="Drag and Drop Mapping" type="sequence" xmlns="http://www.multi-access.de"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.multi-access.de ../../../../MINT-core/lib/MINT-core/model/mim/mint-mappings-2013.xsd">
  <operator id = "1" type="sequence">
    <observations>
      <observation id="2"   process="continuous">
        <state interactor="Interactor.Button" states="pressed" result="b"/>
      </observation>
      <observation id="3" process="instant">
        <state interactor="Interactor.AIO.AIMultiChoice" states="focused"  result="src"/>
      </observation>
      <observation id="4" process="instant">
        <interactor id="5" call="choosen_aios" interactor="Interactor.AIO.AIMultiChoice" name="src" result="aios"/>
      </observation>
    </observations>
    <actions>
      <event id="6" type="drag" target="aois"/>
      <mapping type="sequence" >
        <operator id = "7" type="sequence">
          <observations>
            <observation id="8" process="continuous">
              <state interactor="Interactor.Button" name="b" states="released"/>
            </observation>
            <observation id="9" process="instant">
              <state interactor="Interactor.AIO.AIMultiChoice" states="focused" result="dst"/>
            </observation>
          </observations>
          <actions>
            <interactor id="10" call="drop" interactor="Interactor.AIO.AIMultiChoice" name="dst" parameter="aios"/>
          </actions>
        </operator>
        <fail>
          <interactor id="11" call="drop" interactor="Interactor.AIO.AIMultiChoice" name="src" parameter="aios"/>
        </fail>
      </mapping>
    </actions>
  </operator>
</mapping>
