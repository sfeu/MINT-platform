<mapping name="Reset Execution" xmlns="http://www.multi-access.de"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.multi-access.de ../../../MINT-core/lib/MINT-core/model/mim/mint-mappings.xsd">

  <operator id="1" type="complementary">
    <observations>
      <observation id="2" interactor="Interactor.AIO.AIIN.AIINDiscrete.AICommand" name="reset" states="activated" result="c" process="onchange" />
    </observations>
    <actions>
      <backend id="3" call="reset" class="ExampleApp" parameter="c"/>
    </actions>
  </operator>

</mapping>

