<mapping name="LearnCycle-Progressbar" xmlns="http://www.multi-access.de"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.multi-access.de ../../../../MINT-core/lib/MINT-core/model/mim/mint-mappings.xsd">
  <operator id = "1" type="sequential">
    <observations>
      <observation id="22" interactor="Interactor.Learningcycle" name="learner" states="ticking" process="onchange"/>
      <observation id="23" interactor="Interactor.AIO.AIOUT.AIOUTContinuous" name="progress" states="presenting" process="instant"/>

    </observations>
    <actions>
      <bind id="33" interactor_in="Interactor.Learningcycle" name_in="learner" attr_in="data" interactor_out="Interactor.AIO.AIOUT.AIOUTContinuous" name_out="progress" attr_out="data"/>
    </actions>
  </operator>

</mapping>
