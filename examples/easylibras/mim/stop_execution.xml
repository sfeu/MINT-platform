<mapping name="EasyLIBRAS stop Execution" xmlns="http://www.multi-access.de"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.multi-access.de ../../../MINT-core/lib/MINT-core/model/mim/mint-mappings.xsd">

  <operator id="1" type="sequential">
    <observations>
      <observation id="2" interactor="Interactor.AIO.AIIN.AIINDiscrete.AIReference.AICommand" name="start" states="deactivated" result="c" process="onchange" />
      <observation id="3" interactor="Interactor.Learningcycle" name="learner"  states="learning_cycle" result="learner" process="instant"/>
    </observations>
    <actions>
      <event id="5" type="stop" target="learner"/>
    </actions>
  </operator>

</mapping>
