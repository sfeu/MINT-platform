<mapping name="Right Button click to AISinglePresence prev" xmlns="http://www.multi-access.de"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.multi-access.de ../../../MINT-core/lib/MINT-core/model/mim/mint-mappings.xsd">

  <operator id="1" type="sequential">
    <observations>
      <observation id="2" interactor="Interactor.Pointer.Mouse" name="mouse" states="right_pressed" result="mouse" process="onchange" />
      <observation id="3" interactor="Interactor.AIO.AIOUT.AIContainer.AISinglePresence" name="sheets" states="entered" result="ais" process="instant" />
    </observations>
    <actions>
      <event id="4" type="prev" target="ais"/>
    </actions>
  </operator>

</mapping>
