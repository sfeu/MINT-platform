<mapping name="Sync AICommand activate to ToggleButton select" xmlns="http://www.multi-access.de" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.multi-access.de mint-mappings.xsd">
  <operator type="sequential" id="1">
    <observations>
      <observation id="2" interactor="Interactor.AIO.AIIN.AIINDiscrete.AIReference.AICommand" states="activated" result="aio" process="onchange"/>
      <observation id="3" interactor="Interactor.CIO.HTMLWidget.RadioButton.ToggleButton" name="aio.name" states="unselected" result="cio" process="instant"/>
    </observations>
    <actions>
      <event id="4" type="select" target="cio"/>
    </actions>
  </operator>
</mapping>

