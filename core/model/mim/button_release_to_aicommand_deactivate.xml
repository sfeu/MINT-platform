<mapping name="Sync Button release to AICommand deactivate" xmlns="http://www.multi-access.de" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.multi-access.de mint-mappings.xsd">
  <operator type="sequential" id="1">
    <observations>
      <observation id="2" interactor="Interactor.CIO.Button" states="released" result="cio" process="onchange"/>
      <observation id="3" type="negation" interactor="Interactor.AIO.AIIN.AIINDiscrete.AIReference.AICommand" name="cio.name" states="deactivated" result="aio" process="instant"/>
    </observations>
    <actions>
      <event id="4" type="deactivate" target="aio"/>
    </actions>
  </operator>
</mapping>
