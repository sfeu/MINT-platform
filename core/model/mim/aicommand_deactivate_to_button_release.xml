<mapping name="Sync AICommand deactivate to Button release" xmlns="http://www.multi-access.de" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.multi-access.de mint-mappings.xsd">
  <operator type="sequential" id="1">
    <observations>
      <observation id="2" interactor="Interactor.AIO.AIIN.AIINDiscrete.AIReference.AICommand" states="deactivated" result="aio" process="onchange"/>
      <observation id="3" type="negation" interactor="Interactor.CIO.Button" name="aio.name" states="released" result="cio" process="instant"/>
    </observations>
    <actions>
      <event id="4" type="release" target="cio"/>
    </actions>
  </operator>
</mapping>

